name _dict = {
            "КРИНЖ: "что-то стыдное"",
            "ЛОЛ: "что-то смешное"",
            "ЩИЩ: "одобрение"",
            "КРИПОВЫЙ: "страшный"",
            "АГРИТЬСЯ: "раздражаться""
}
word = input("Введите непонятное слово капс локом: ")
if word in name _dict.keys():
    print(name _dict[word])
else:
    print("Мы такого не знаем")
